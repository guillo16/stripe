<h3 class="home-title-border"></h3>
<p class="home-title-footer">Perfect bags for your backyard adventures.</p>
<div class="homepage-wrapper">
  <section id="home-products">
    <div class="home-product-container">
      <% @products.uniq(&:title).first(4).each do |product| %>
      <%= render 'shared/cards', product: product%>
      <% end %>
    </div>
  </section>
  <section id="db-black">
    <%= render 'db_black' %>
  </section>
  <h2 class="home-title"> Categories</h2>
  <h3 class="home-title-border"></h3>
  <section id="home-categories">
    <%= render 'categories' %>
  </section>
  <section id='credit-cards'>
    <%= render 'credit_cards' %>
  </section>
